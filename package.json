    "@types/typography": "^0.16.2",
    "babel-jest": "^24.8.0",
    "babel-preset-gatsby": "^0.2.5",
    "cross-env": "^5.2.0",
    "cypress": "^3.4.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.8.0",
    "prettier": "^1.18.2",
    "start-server-and-test": "^1.9.1",
    "ts-jest": "^24.0.2",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-loader": "^3.5.4",
